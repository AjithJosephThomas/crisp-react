{
  "name": "crisp-react-server",
  "version": "1.0.0",
  "description": "Express server for crisp-react",
  "license": "MIT",
  "keywords": [
    "NodeJS",
    "Express",
    "Typescript",
    "Jest",
    "Supertest",
    "client-server",
    "client server",
    "VS Code",
    "starter project",
    "sample"
  ],
  "scripts": {
    "lint": "tslint -p .",
    "compile": "tsc -p .",
    "compile:watch": "tsc -w -p .",
    "precopy": "rimraf build/client && mkdirp build/client",
    "copy": "copyfiles -f ../client/dist/* build/client/",
    "prestart": "yarn copy && yarn compile",
    "start": "node ./build/js/main.js",
    "prestart:prod": "yarn prestart",
    "start:prod": "cross-env NODE_ENV=production node ./build/js/main.js",
    "prod": "yarn start:prod",
    "prestart:dev": "yarn prestart",
    "start:dev": "cross-env USE_DEV_WEBSERVER=true node ./build/js/main.js",
    "dev": "yarn start:dev",
    "test": "cross-env NODE_ENV=test jest"
  },
  "dependencies": {
    "express": "4.17.1",
    "node-fetch": "^2.6.0",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/express": "4.17.1",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.2",
    "@types/node-fetch": "2.5.0",
    "@types/supertest": "^2.0.8",
    "copyfiles": "^2.1.1",
    "cross-env": "^5.2.0",
    "jest": "24.9.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "tslib": "1.10.0",
    "tslint": "5.19.0",
    "typescript": "3.6.2"
  }
}
